table 'Time Intelligence'
	lineageTag: 351b5797-7e24-42c9-a7ad-b76bea034ad2

	calculationGroup

		calculationItem MTD = CALCULATE(SELECTEDMEASURE(), DATESMTD('dimdate'[Date]))

		calculationItem QTD = CALCULATE(SELECTEDMEASURE(), DATESQTD('dimdate'[Date]))

		calculationItem YoY = CALCULATE(SELECTEDMEASURE(), SAMEPERIODLASTYEAR('dimdate'[Date]))

		calculationItem YTD = CALCULATE(SELECTEDMEASURE(), DATESYTD('dimdate'[Date]))

		calculationItem YoY% = ```
				
				var CurrentMeasure = SELECTEDMEASURE() 
				var Prior = CALCULATE(SELECTEDMEASURE(), SAMEPERIODLASTYEAR('dimdate'[Date]))
				RETURN DIVIDE(CurrentMEasure - Prior, Prior)
				
				```

